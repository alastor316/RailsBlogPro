<% if !notice.nil?%>
  
    <div class="alert alert-dismissible  ">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
            <%=notice%>
    </div>
<%end%>    
<% if !alert.nil?%>
    <div class="alert alert-dismissible alert-warning padding-Alert">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <h4 class="alert-heading">Warning!</h4>
                <p><%=alert%></p>
    </div>
<%end%>    
